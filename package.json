{
  "name": "thinkkoa-application",
  "description": "application created by thinkkoa",
  "version": "1.0.0",
  "scripts": {
    "test": "npm run eslint",
    "start": "npm run compile && supervisor -w app -q index",
    "compile": "babel src -d app -D",
    "watch-compile": "npm run compile -- --watch",
    "eslint": "eslint src"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": true
          },
          "loose": true
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "ip": "^1.1.5",
    "thinkkoa": "^1.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "cheerio": "^1.0.0-rc.1",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.3.0",
    "mocha": "^3.4.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "supertest": "^3.0.0",
    "supervisor": "^0.12.0",
    "think_model": "^1.0.8"
  }
}
